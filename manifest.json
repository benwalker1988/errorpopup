{
    "name": "thinkTRIBE Error Popup",
    "description": "Preview errors on the wallboard",
    "version": "1.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://portal.thinktribe.com/*/clientWallboard*",
                "https://portal-staging.thinktribe.com/*/clientWallboard*"
            ],
            "js": [
                "jquery-3.6.0.min.js",
                "jquery.hoverIntent.min.js",
                "moment.js",
                "moment-timezone-with-data-10-year-range.js",
                "error-popup.js"
            ],
            "css": [
                "error-popup.css"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {}
}
